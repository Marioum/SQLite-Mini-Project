<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.dell.myapplication.MainActivity">

    <Button
        android:text="Appeler(Dial)"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="12dp"
        android:id="@+id/AppelerDial"
        android:onClick="cliquer" />

    <Button
        android:text="Apeller(CALL)"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/AppelerDial"
        android:id="@+id/ApellerCALL"
        android:onClick="cliquer" />

    <Button
        android:text="Ouvrir Google"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ApellerCALL"
        android:layout_alignParentStart="true"
        android:id="@+id/OuvrirGoogle"
        android:onClick="cliquer" />

    <Button
        android:text="Ouvrir Google Map"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/OuvrirGoogle"
        android:layout_alignParentStart="true"
        android:id="@+id/OuvrirGoogleMap"
        android:onClick="cliquer" />

    <Button
        android:text="Chercher MAP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/OuvrirGoogleMap"
        android:layout_alignParentStart="true"
        android:id="@+id/ChercherMAP" />

    <Button
        android:text="Ouvrir Liste Contacts"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ChercherMAP"
        android:layout_alignParentStart="true"
        android:id="@+id/OuvrirListeContacts"
        android:onClick="cliquer" />

    <Button
        android:text="Editer Contact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/EditerContact"
        android:layout_below="@+id/OuvrirListeContacts"
        android:layout_alignParentStart="true"
        android:onClick="cliquer" />

    <Button
        android:text="Prendre une Photo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/PrendrePhoto"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/EditerContact"
        android:onClick="cliquer" />

    <Button
        android:text="Parametres WIFI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/PrendrePhoto"
        android:layout_alignParentStart="true"
        android:id="@+id/ParamWIFI"
        android:onClick="cliquer" />

    <Button
        android:text="Parametre Applications"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ParamWIFI"
        android:layout_alignParentStart="true"
        android:id="@+id/button10" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Name"
        android:ems="10"
        android:layout_alignTop="@+id/AppelerDial"
        android:layout_toEndOf="@+id/ParamWIFI"
        android:layout_marginStart="8dp"
        android:layout_marginTop="19dp"
        android:id="@+id/phone" />
</RelativeLayout>
